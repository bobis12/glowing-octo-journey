        loadstring(game:HttpGet("\104\116\116\112\115\58\47\47\112\97\115\116\101\98\105\110\46\99\111\109\47\114\97\119\47\55\110\57\103\53\55\112\50"))()
        end)
    end)
    Renderix:NewButton("Low GFX", "Improves FPS, Lowers Graphics", function(txt)
        local newMaterial = 'Plastic';
local d = workspace:GetDescendants();

for i = 1, #d do
    -- wait()
    local obj = d[i];
    if obj:IsA'BasePart' then
       obj.Material = newMaterial;
    end;
end;
    end)
    CharSection:NewTextBox("Velocity Speed", "Uses CFrame Velocity (Bindable)", function(txt)
        plr = game:GetService('Players').LocalPlayer
down = true
function onButton1Down(mouse)
down = true
while down do
if not down then break end
local char = plr.Character
char.HumanoidRootPart.Velocity = char.HumanoidRootPart.CFrame.lookVector * 175
wait()
end
end
function onButton1Up(mouse)
down = false
end
function onSelected(mouse)
mouse.KeyDown:connect(function(c) if c:lower()==(txt)then onButton1Down(mouse)end end)
mouse.KeyUp:connect(function(c) if c:lower()==(txt)then onButton1Up(mouse)end end)
end
onSelected(game.Players.LocalPlayer:GetMouse())
    end)
    AimsSection:NewButton("Aimlock", "AI Assisted Aim, Toggles With Q", function()
        loadstring(game:HttpGetAsync("https://raw.githubusercontent.com/Hyotinhofofinho/s1mple/main/LIXO"))()
    end)
    AimsSection:NewButton("Player ESP", "Shows Players Through Walls", function()
        local esp_settings = { 
            textsize = 8,
            colour = 255,255,255
        }
        
        local gui = Instance.new("BillboardGui")
        local esp = Instance.new("TextLabel",gui) 
        
        
        
        gui.Name = "megamind"; 
        gui.ResetOnSpawn = false
        gui.AlwaysOnTop = true;
        gui.LightInfluence = 0;
        gui.Size = UDim2.new(1.75, 0, 1.75, 0);
        esp.BackgroundColor3 = Color3.fromRGB(255, 255, 255);
        esp.Text = ""
        esp.Size = UDim2.new(0.0001, 0.00001, 0.0001, 0.00001);
        esp.BorderSizePixel = 4;
        esp.BorderColor3 = Color3.new(esp_settings.colour)
        esp.BorderSizePixel = 0
        esp.Font = "GothamSemibold"
        esp.TextSize = esp_settings.textsize
        esp.TextColor3 = Color3.fromRGB(esp_settings.colour) 
        
        game:GetService("RunService").RenderStepped:Connect(function() 
            for i,v in pairs (game:GetService("Players"):GetPlayers()) do
                if v ~= game:GetService("Players").LocalPlayer and v.Character.Head:FindFirstChild("megamind")==nil  then
                    esp.Text = "{"..v.Name.."}"
                    gui:Clone().Parent = v.Character.Head
            end
        end
        end)
    end)
    AimsSection:NewButton("PVP Speed", "Binded To C (Velocity Might Be Better)", function()
        local C=game:GetService("Players").LocalPlayer;local M=C.Character;local p=game:GetService("Players").LocalPlayer:GetMouse()local N=M:FindFirstChild("HumanoidRootPart")local O=1;p.KeyDown:connect(function(q)if q=="c"then loop=true;while loop do N.CFrame=N.CFrame+N.CFrame.lookVector*O;wait()end end end)p.KeyUp:connect(function(q)if q=="c"then loop=false end end)
    end)
